{
	"manifest_version": 2,
	"name": "Dog Whistle Sniffer",
	"version": "1.0",
	"description": "Replaces dog whistles with their coded meaning.",
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["jquery-3.1.1.min.js", "iancaro.js"],
			"css": ["word.css"],
			"run_at": "document_end"
		}
	],
"permissions": [
          "fontSettings"
        ]
}

